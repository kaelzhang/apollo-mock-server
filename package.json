  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/apollo-mock-server/issues"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "codecov": "^3.3.0",
    "nyc": "^13.3.0",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint": "^5.16.0"
  },
  "dependencies": {
    "core-util-is": "^1.0.2",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "uuid": "^3.3.2"
  }
